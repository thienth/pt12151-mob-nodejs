<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <table>
    	<thead>
    		<tr>
    			<th>ID</th>
    			<th>User name</th>
    			<th>Image</th>
    			<th></th>
    		</tr>
    	</thead>
    	<tbody>
    		<%
    			user.forEach(function(u){
    				%>
    				<tr>
    			        <td><%= u.id%></td>
    			        <td><%= u.username%></td>
    			        <td>
    			        	<img src="<%= u.avatar%>" width=200>
    			        </td>
    		        </tr>

    				<%
    			})
    		%>
    		
    	</tbody>
    </table>
  </body>
</html>
